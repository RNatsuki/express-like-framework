{
  "name": "@rnatsuki/express-lite",
  "type": "module",
  "version": "1.0.0",
  "description": "A lightweight Express-like HTTP framework for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "stress-test": "node scripts/stress-test.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.ts"]
  },
  "keywords": ["express", "framework", "http", "server", "node", "lightweight"],
  "author": "Ibarra",
  "license": "ISC",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RNatsuki/express-like-framework.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@10.6.1",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "autocannon": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  }
}
